import math
n=int(input())
pc=0
dc=0
x=int(math.sqrt(n))
for i in range(2,x+1):
    if n%i==0:
        print("Not a prime")
        break
else:
    while n:
        r=n%10
        n=n//10
        dc+=1
        y=int(math.sqrt(r))
        for i in range(2,y+1):
            if r%i==0:
                break
        else:
            pc+=1
    if pc==dc:
        print("Mega prime")
    else:
        print("Prime but not a mega prime")
