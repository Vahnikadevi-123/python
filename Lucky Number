#*****************
n=int(input())
temp=n
digit_count=0
divide_count=0
while n:
    r=n%10
    n=n//10
    digit_count+=1
    if temp%r==0:
        divide_count+=1
    else:
        print("Unlucky Number")
        break
 else:
     print("LUcky Number")
    


