n=int(input())
b=n
while n:
    r=n%10 
    a=0
    n=n//10
    a+=r
    if b%a==0:
        continue
    print("unlucky number")
    break
else:
    print("Lucky Number")
    
 
 
#OR



n=int(input())
temp=n
digit_count=0
division_count=0
while n!=0:
    r=n%10
    digit_count+=1
    if temp%r==0:
        division_count+=1
    n=n//10
if digit_count==division_count:
    print("Lucky Number")
else:
    print("Unlucky Number")

