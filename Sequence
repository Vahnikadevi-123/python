n=int(input())
s=True
i=1
while i:
    if s==True:
        print(i,end=" ")
        i+=1
    if i==n:
        s=False
    if s==False:
        print(i,end=" ")
        i-=1   
        
        
        

n=int(input())
i=1
while n:
    if i<=n-1:
        print(i,end=" ")
        i+=1
    else:
        print(n,end=" ")
        n-=1
